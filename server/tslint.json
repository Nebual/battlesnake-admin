{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "object-literal-shorthand": false
  },
  "typeCheck": true,
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["src/typings/**/*.d.ts"]
  }
}
